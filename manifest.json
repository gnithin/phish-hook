{
  "manifest_version": 2,
  "name": "Phish hook",
  "version": "0.0.1",
  "description": "This is a sample description",
  "short_name": "Phook",
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "src/content_scripts/content_scripts.css"
      ],
      "js": [
        "src/content_scripts/content_scripts.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Phook",
    "default_popup": "src/browser_action/browser_action.html"
  }
}